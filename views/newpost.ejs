<div class="new-post">
	<form style="margin:0 5px;">
		<input class="ng-pristine ng-valid md-input" ng-model="post.url" placeholder="url" type="text" />
		<input class="ng-pristine ng-valid md-input" ng-model="post.title" placeholder="title" type="text" />
		<button class="md-button md-ink-ripple" ng-click="addPost()">
			Add Post
		</button>
	</form>
</div>